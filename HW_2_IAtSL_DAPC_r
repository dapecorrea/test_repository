        "set.seed(123) #initialize the data\n",
        "#PCR\n",
        "pcr_model <- pcr(medv ~ ., data=Boston, scale=TRUE, validation=\"CV\")\n",
        "validationplot(pcr_model, val.type=\"MSEP\")\n",
        "optimal_comp <- which.min(pcr_model$validation$PRESS)"
