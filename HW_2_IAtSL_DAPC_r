    {
      "cell_type": "markdown",
      "source": [
        "## 4.) Compare the PCR model‚Äôs performance to that of a regular linear regression model on the same dataset."
      ],
      "metadata": {
        "id": "d0OhnP86iDpx"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Comparison using K-fold CV with a K=10\n",
        "\n",
        "# Set seed for reproducibility\n",
        "set.seed(123)\n",
        "\n",
        "# Prepare data\n",
        "data(Boston)\n",
        "predictors <- Boston[, -which(names(Boston) == \"medv\")]\n",
        "response <- Boston$medv\n",
        "\n",
        "# Create 10-fold CV indices\n",
        "folds <- createFolds(response, k = 10)\n",
        "\n",
        "# Function to calculate MSE and R-squared\n",
        "calc_metrics <- function(actual, predicted) {\n",
        "  mse <- mean((actual - predicted)^2)\n",
        "  r2 <- 1 - sum((actual - predicted)^2) / sum((actual - mean(actual))^2)\n",
        "  return(c(mse = mse, r2 = r2))\n",
        "}\n",
        "\n",
        "# Initialize results storage\n",
        "results <- data.frame(\n",
        "  fold = 1:10,\n",
        "  pcr_mse = numeric(10),\n",
        "  pcr_r2 = numeric(10),\n",
        "  lm_mse = numeric(10),\n",
        "  lm_r2 = numeric(10)\n",
        ")\n",
        "\n",
        "# Perform 10-fold CV\n",
        "for (i in 1:10) {\n",
        "  # Create train-test splits\n",
        "  test_indices <- folds[[i]]\n",
        "  train_data <- Boston[-test_indices, ]\n",
        "  test_data <- Boston[test_indices, ]\n",
        "\n",
        "  # Fit PCR model (using optimal components from previous task)\n",
        "  pcr_model <- pcr(medv ~ ., data = train_data, scale = TRUE, validation = \"none\")\n",
        "\n",
        "  # Fit linear regression\n",
        "  lm_model <- lm(medv ~ ., data = train_data)\n",
        "\n",
        "  # Make predictions\n",
        "  pcr_pred <- predict(pcr_model, newdata = test_data, ncomp = 10) # Using all components\n",
        "  lm_pred <- predict(lm_model, newdata = test_data)\n",
        "\n",
        "  # Calculate metrics\n",
        "  pcr_metrics <- calc_metrics(test_data$medv, pcr_pred)\n",
        "  lm_metrics <- calc_metrics(test_data$medv, lm_pred)\n",
        "\n",
        "  # Store results\n",
        "  results[i, \"pcr_mse\"] <- pcr_metrics[\"mse\"]\n",
        "  results[i, \"pcr_r2\"] <- pcr_metrics[\"r2\"]\n",
        "  results[i, \"lm_mse\"] <- lm_metrics[\"mse\"]\n",
        "  results[i, \"lm_r2\"] <- lm_metrics[\"r2\"]\n",
        "}\n",
        "\n",
        "# Aggregate results\n",
        "summary_results <- data.frame(\n",
        "  Model = c(\"PCR\", \"Linear Regression\"),\n",
        "  Avg_MSE = c(mean(results$pcr_mse), mean(results$lm_mse)),\n",
        "  SD_MSE = c(sd(results$pcr_mse), sd(results$lm_mse)),\n",
        "  Avg_R2 = c(mean(results$pcr_r2), mean(results$lm_r2)),\n",
        "  SD_R2 = c(sd(results$pcr_r2), sd(results$lm_r2))\n",
        ")\n",
        "\n",
        "# Print results\n",
        "print(summary_results)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "id": "rDFdE27jiFNC",
        "outputId": "a28f327d-e478-4c80-f801-8a396016c917"
      },
      "execution_count": 65,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "              Model  Avg_MSE   SD_MSE    Avg_R2     SD_R2\n",
            "1               PCR 26.86926 11.58174 0.6776814 0.1330790\n",
            "2 Linear Regression 24.18997 10.72594 0.7120418 0.1161447\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Paired t-test to check for significant differences\n",
        "t_test_mse <- t.test(results$pcr_mse, results$lm_mse, paired = TRUE)\n",
        "t_test_r2 <- t.test(results$pcr_r2, results$lm_r2, paired = TRUE)\n",
        "\n",
        "cat(\"\\nPaired t-test results for MSE:\")\n",
        "print(t_test_mse)\n",
        "\n",
        "cat(\"\\nPaired t-test results for R-squared:\")\n",
        "print(t_test_r2)"
      ],
