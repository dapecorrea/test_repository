    {
      "cell_type": "code",
      "source": [
        "# Visualization\n",
        "library(ggplot2)\n",
        "library(tidyr)\n",
        "\n",
        "# Prepare data for plotting\n",
        "plot_data <- gather(results, key = \"metric\", value = \"value\", -fold)\n",
        "plot_data$model <- ifelse(grepl(\"pcr\", plot_data$metric), \"PCR\", \"Linear Regression\")\n",
        "plot_data$metric_type <- ifelse(grepl(\"mse\", plot_data$metric), \"MSE\", \"R-squared\")\n",
        "\n",
        "# Plot MSE comparison\n",
        "ggplot(plot_data[plot_data$metric_type == \"MSE\", ],\n",
        "       aes(x = model, y = value, fill = model)) +\n",
        "  geom_boxplot() +\n",
        "  labs(title = \"Comparison of MSE between PCR and Linear Regression\",\n",
        "       y = \"Mean Squared Error\", x = \"Model\") +\n",
        "  theme_minimal()"
      ],
