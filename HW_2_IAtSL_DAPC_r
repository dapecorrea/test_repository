          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Convert to numeric for comparison (exclude intercept)\n",
        "ridge_coef_vals <- as.vector(coef(ridge_cv, s = \"lambda.min\"))[-1]\n",
        "lasso_coef_vals <- as.vector(coef(lasso_cv, s = \"lambda.min\"))[-1]\n",
        "names(ridge_coef_vals) <- colnames(x_scaled)\n",
        "\n",
        "# Combine the model results into a data frame\n",
        "shrink_df <- data.frame(\n",
        "  Variable = names(ridge_coef_vals),\n",
        "  Ridge = ridge_coef_vals,\n",
        "  Lasso = lasso_coef_vals\n",
        ")\n",
        "\n",
        "# Add absolute shrinkage and flag for zero coefficients in LASSO\n",
        "shrink_df$Shrink_Diff = abs(shrink_df$Ridge) - abs(shrink_df$Lasso)\n",
        "shrink_df$Lasso_Zero = shrink_df$Lasso == 0\n",
        "\n",
        "# Sort by shrinkage difference\n",
        "shrink_df <- shrink_df[order(-abs(shrink_df$Shrink_Diff)), ]\n",
        "\n",
        "# Print top rows\n",
        "head(shrink_df, 10)"
      ],
      "metadata": {
